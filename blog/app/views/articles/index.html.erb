<h2 class="Header_2">blog</h2>
<table>
  <tr>
    <th><h3>article</h3></th>
    <th><h3>text</h3></th>
    <th></th>
  </tr>

  <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.text %></td>
      <td><%= link_to 'name', article_path(article) %></td>
    </tr>
  <% end %>
</table>
